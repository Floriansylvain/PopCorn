// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Jeux{
  id Int @id @default(autoincrement())
  Title String
  Description String 
  Picture String @db.VarChar(255)
  Workers String
}
